pipeline{
    agent any
    stages{
        stage('Dump Tomcat Threads'){
            steps{
                script{
                    ok = dumpTomcatThreads(
                        samples = "${samples}",
                        logfile = "${logfile}",
                        output = "${output}",
                        inventory = "${inventory}"
                    )
                }
            }
        }
    }
}